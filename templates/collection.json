{
  "sections": {
    "collection-header": {
      "type": "collection--header",
      "settings": {
        "enable-header": true,
        "show-title": false,
        "title-alignment": "center",
        "show-description": true,
        "description-alignment": "with-title",
        "show-image": true,
        "use-image-fallback": false,
        "image-height": "large",
        "alt-color": false,
        "apply-overlay": true,
        "parallax-enabled": true
      }
    },
    "navigation": {
      "type": "collection--navigation",
      "settings": {
        "show-browse-button": true,
        "show-sort-button": true,
        "show-refine-button": true,
        "show-layout-buttons": true,
        "show-collections-list": true,
        "custom-list-collections": "",
        "show-sort-list": true,
        "show-refine-list": true,
        "swatches-enabled": false,
        "show-search-link": true
      }
    },
    "main": {
      "type": "collection--body",
      "settings": {
        "products-per-row--desktop": 4,
        "products-per-row--mobile": "1",
        "number-of-rows": 4
      }
    }
  },
  "order": [
    "collection-header",
    "navigation",
    "main"
